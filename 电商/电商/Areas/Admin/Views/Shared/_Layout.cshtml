<body>

    <section id="container">
        <!-- **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars tooltips" data-placement="right" data-original-title="列表"></div>
            </div>
            <!--logo start-->
            <a href="index.html" class="logo"><b>后台管理</b></a>
            <!--logo end-->

            <div class="top-menu">
                <ul class="nav pull-right top-menu">
                    <li><a class="logout" href="/Admin/Admin/Login">Logout</a></li>
                </ul>
            </div>
        </header>
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">
                    
                    <p class="centered"><a href="profile.html"><img src="~/Other/icon.jpg" class="img-circle" width="60"></a></p>
                    <h5 class="centered">Kojewang</h5>

                    <li class="sub-menu active dcjq-parent-li">
                        <a href="javascript:" id="Product">
                            <i class="fa fa-desktop"></i>
                            <span>商品管理</span>
                        </a>
                        <ul class="sub" >
                            <li id="index"><a href="/Admin/Admin/index">商品列表</a></li>
                            <li id="add"><a href="/Admin/Admin/add">商品添加</a></li>
                        </ul>
                    </li>

                 

                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="fa fa-desktop"></i>
                            <span>类别管理</span>
                        </a>
                        <ul class="sub">
                            <li><a href="/Admin/Admin/listCategory">类别列表</a></li>
                            <li><a href="/Admin/Admin/listCategoryAdd">类别添加</a></li>
                        </ul>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="fa fa-desktop"></i>
                            <span>用户管理</span>
                        </a>
                        <ul class="sub">
                            <li><a href="/Admin/Admin/userlist">用户列表</a></li>
                        </ul>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="fa fa-desktop"></i>
                            <span>订单管理</span>
                        </a>
                        <ul class="sub">
                            <li><a href="/Admin/Admin/orderlist">订单列表</a></li>
                        </ul>
                    </li>
                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        

        <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                @RenderBody()

            </section>
        </section>

        <!--main content end-->
        <!--footer start-->
       
        <!--footer end-->
    </section>
    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/Other/bkt_1_dashgumfree/assets//js/jquery.js"></script>
    <script src="~/Other/bkt_1_dashgumfree/assets//js/jquery-1.8.3.min.js"></script>
