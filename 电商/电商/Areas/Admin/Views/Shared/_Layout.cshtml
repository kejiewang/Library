    <script src="~/Other/bkt_1_dashgumfree/assets//js/jquery.scrollTo.min.js"></script>
    <script src="~/Other/bkt_1_dashgumfree/assets//js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="~/Other/bkt_1_dashgumfree/assets//js/jquery.sparkline.js"></script>


