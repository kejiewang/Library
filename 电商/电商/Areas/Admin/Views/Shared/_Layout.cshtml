    <script type="text/javascript" src="~/Other/bkt_1_dashgumfree/assets//js/gritter-conf.js"></script>

    <!--script for this page-->
    <script src="~/Other/bkt_1_dashgumfree/assets//js/sparkline-chart.js"></script>
    <script src="~/Other/bkt_1_dashgumfree/assets//js/zabuto_calendar.js"></script>


    
    

